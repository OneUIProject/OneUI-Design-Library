<?xml version="1.0" encoding="utf-8" ?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="false" android:state_checked="true" android:drawable="@drawable/sesl_btn_check_to_on_025" />
	<item android:state_enabled="false" android:drawable="@drawable/sesl_btn_check_to_on_001" />
	<item android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/sesl_btn_check_to_on_026" />
	<item android:state_checked="false" android:id="@id/unchecked" android:drawable="@drawable/sesl_btn_check_to_on_000" />
	<transition android:drawable="@drawable/sesl_btn_check_to_on_024" android:toId="@id/checked" android:fromId="@id/unchecked" />
	<transition android:drawable="@drawable/sesl_btn_check_to_on_002" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>
