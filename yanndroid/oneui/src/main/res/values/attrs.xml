<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="progressActivatedColor" format="reference|color" />
    <attr name="progressNormalColor" format="reference|color" />
    <attr name="colorControlActivated" format="color" />
    <attr name="colorControlNormal" format="color" />


    <attr name="seslSwitchBarBackgroundActivatedColor" format="color" />
    <attr name="seslSwitchBarBackgroundColor" format="color" />
    <attr name="seslSwitchBarStyle" format="reference" />
    <attr name="seslSwitchBarTextActivatedColor" format="color" />
    <attr name="seslSwitchBarTextColor" format="color" />

    <attr name="useDisabledAlpha" format="boolean" />

    <attr name="layout_isTitleCustom" format="boolean" />

    <attr name="collapsingToolbarLayoutStyle" format="reference" />

    <attr name="seslSeekBarMode">
        <enum name="expand" value="5" />
        <enum name="expand_vertical" value="6" />
        <enum name="split" value="4" />
        <enum name="standard" value="0" />
        <enum name="vertical" value="3" />
        <enum name="warning" value="1" />
    </attr>
    <attr name="seslThumbRadius" format="dimension" />
    <attr name="seslTrackMaxWidth" format="dimension" />
    <attr name="seslTrackMinWidth" format="dimension" />

    <attr name="checkMarkGravity">
        <flag name="end" value="0x00800005" />
        <flag name="left" value="0x00000003" />
        <flag name="right" value="0x00000005" />
        <flag name="start" value="0x00800003" />
    </attr>

    <attr name="popupEnterTransition" format="reference" />
    <attr name="popupExitTransition" format="reference" />


    <!--custom views-->
    <attr name="text" format="string" />
    <attr name="image" format="reference|color" />
    <attr name="selected" format="boolean" />

    <declare-styleable name="SplashViewSimple">
        <attr name="text" />
        <attr name="image" />
    </declare-styleable>

    <declare-styleable name="SplashViewAnimated">
        <attr name="text" />
        <attr name="foreground_image" format="reference|color" />
        <attr name="background_image" format="reference|color" />
        <attr name="animation" format="reference" />
    </declare-styleable>

    <declare-styleable name="OptionButton">
        <attr name="icon" />
        <attr name="text" />
        <attr name="selected" />
        <attr name="counter" format="integer" />
        <attr name="counterEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="OptionGroup">
        <attr name="selectedOptionButton" format="reference" />
    </declare-styleable>

    <declare-styleable name="DrawerLayout">
        <attr name="toolbar_title" format="string" />
        <attr name="toolbar_subtitle" format="string" />
        <attr name="toolbar_expandable" format="boolean" />
        <attr name="toolbar_expanded" format="boolean" />
        <attr name="drawer_icon" format="reference|color" />
        <attr name="drawer_viewId" format="reference|enum" />
    </declare-styleable>

    <declare-styleable name="ToolBarLayout">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="navigationIcon" />
        <attr name="expandable" format="boolean" />
        <attr name="expanded" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AboutPage">
        <attr name="optional_text" format="string" />
    </declare-styleable>

    <declare-styleable name="RelatedCard">
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="ToolbarImageButton">
        <attr name="seslTooltipText" format="string" />
    </declare-styleable>

    <declare-styleable name="Switch">
        <attr name="android:thumb" />
        <attr name="thumbTint" format="color" />
        <attr name="thumbTintMode" />
        <attr name="track" format="reference" />
        <attr name="trackTint" format="color" />
        <attr name="trackTintMode"/>
        <attr name="android:textOn" />
        <attr name="android:textOff" />
        <attr name="thumbTextPadding" format="dimension" />
        <attr name="switchTextAppearance" format="reference" />
        <attr name="switchMinWidth" format="dimension" />
        <attr name="switchPadding" format="dimension" />
        <attr name="splitTrack" format="boolean" />
        <attr name="showText" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SamsungCoordinatorLayout">
        <attr name="keylines" format="reference"/>
        <attr name="statusBarBackground" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="SamsungCoordinatorLayout_Layout">
        <attr name="android:layout_gravity"/>
        <attr name="layout_behavior" format="string"/>
        <attr name="layout_anchor" format="reference"/>
        <attr name="layout_keyline" format="integer"/>
        <attr name="layout_anchorGravity"/>
        <attr name="layout_insetEdge" />
        <attr name="layout_dodgeInsetEdges" />
    </declare-styleable>
    <declare-styleable name="SamsungAppBarLayout">
        <attr name="elevation"/>
        <attr name="android:background"/>
        <attr name="expanded"/>
        <attr name="android:keyboardNavigationCluster"/>
        <attr name="android:touchscreenBlocksFocus"/>
        <attr name="liftOnScroll" format="boolean"/>
        <attr name="liftOnScrollTargetViewId" format="reference"/>
        <attr name="statusBarForeground" format="color"/>
        <attr name="sesl_layout_heightPercent" format="float" />
        <attr name="android:paddingBottom"/>
    </declare-styleable>
    <declare-styleable name="SamsungAppBarLayout_Layout">
        <attr name="layout_scrollFlags" />
        <attr name="layout_scrollInterpolator" format="reference"/>
    </declare-styleable>
    <declare-styleable name="SamsungCollapsingToolbarLayout">
        <attr name="expandedTitleMargin" format="dimension"/>
        <attr name="expandedTitleMarginStart" format="dimension"/>
        <attr name="expandedTitleMarginTop" format="dimension"/>
        <attr name="expandedTitleMarginEnd" format="dimension"/>
        <attr name="expandedTitleMarginBottom" format="dimension"/>
        <attr name="expandedTitleTextAppearance" format="reference"/>
        <attr name="extendTitleEnabled" format="boolean" />
        <attr name="extendTitleTextAppearance" format="reference" />
        <attr name="extendSubTitleTextAppearance" format="reference" />
        <attr name="collapseTitleTextAppearance" format="reference" />
        <attr name="collapsedTitleTextAppearance" format="reference"/>
        <attr name="contentScrim" format="color"/>
        <attr name="statusBarScrim" format="color" />
        <attr name="toolbarId" format="reference"/>
        <attr name="scrimVisibleHeightTrigger" format="dimension"/>
        <attr name="scrimAnimationDuration" format="integer"/>
        <attr name="collapsedTitleGravity" />
        <attr name="expandedTitleGravity" />
        <attr name="titleEnabled" format="boolean"/>
        <attr name="title"/>
        <attr name="subtitle" />
    </declare-styleable>
    <declare-styleable name="SamsungCollapsingToolbarLayout_Layout">
        <attr name="layout_collapseMode" />
        <attr name="layout_collapseParallaxMultiplier" format="float"/>
        <attr name="layout_isTitleCustom" />
    </declare-styleable>

</resources>