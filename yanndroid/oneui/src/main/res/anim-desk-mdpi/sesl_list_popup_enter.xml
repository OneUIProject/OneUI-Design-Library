<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:shareInterpolator="false">
    <scale
        android:duration="250"
        android:fromXScale="79.99999%"
        android:fromYScale="79.99999%"
        android:interpolator="@interpolator/sine_in_out_80"
        android:pivotX="50.0%"
        android:toXScale="100.0%"
        android:toYScale="100.0%" />
    <alpha
        android:duration="150"
        android:fromAlpha="0.0"
        android:toAlpha="1.0" />
</set>